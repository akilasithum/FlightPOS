<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.pos.swoop">

    <!-- To auto-complete the email text field in the login form with the user's emails -->
    <uses-permission android:name="android.permission.GET_ACCOUNTS" />
    <uses-permission android:name="android.permission.READ_PHONE_STATE" />
    <uses-permission android:name="android.permission.READ_PROFILE" />
    <uses-permission android:name="android.permission.READ_CONTACTS" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.VIBRATE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.CHANGE_WIFI_STATE" />
    <uses-permission android:name="android.permission.NFC" />
    <uses-permission android:name="android.permission.RECEIVE_BOOT_COMPLETED" />
    <uses-permission android:name="android.permission.CAMERA" />
    <uses-feature android:name="android.hardware.camera" />
    <uses-feature android:name="android.hardware.camera.autofocus" />
    <uses-permission android:name="com.sunmi.perm.LED" />
    <uses-permission android:name="com.sunmi.perm.MSR" />
    <uses-permission android:name="com.sunmi.perm.ICC" />
    <uses-permission android:name="com.sunmi.perm.PINPAD" />
    <uses-permission android:name="com.sunmi.perm.SECURITY" />
    <uses-permission android:name="com.sunmi.perm.CONTACTLESS_CARD" />

    <uses-feature
        android:name="android.hardware.nfc"
        android:required="true" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher"
        android:supportsRtl="true"
        android:theme="@style/Background">
        <receiver
            android:name="com.pos.swoop.BootUpReceiver"
            android:enabled="true"
            android:permission="android.permission.RECEIVE_BOOT_COMPLETED">
            <intent-filter>
                <action android:name="android.intent.action.BOOT_COMPLETED" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </receiver>

        <activity
            android:name="com.pos.swoop.SplashActivity"
            android:noHistory="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.pos.swoop.LoginActivity"
            android:label="@string/app_name"
            android:theme="@style/Background" />
        <activity
            android:name="com.pos.swoop.MainActivity"
            android:label="@string/title_activity_main" />
        <activity
            android:name="com.pos.swoop.FlightAttendentLogin"
            android:label="@string/title_activity_flight_attendent_login"
            android:theme="@style/Background" />
        <activity
            android:name="com.pos.swoop.AttendendMainActivity"
            android:label="Select Flight Details"
            android:windowSoftInputMode="adjustPan"/>
        <activity
            android:name="com.pos.swoop.SyncActivity"
            android:label="Sync Data From Server" />

        <service
            android:name="com.amazonaws.mobileconnectors.s3.transferutility.TransferService"
            android:enabled="true" />

        <activity android:name="com.pos.swoop.PrintActivity" />
        <activity android:name="com.pos.swoop.AttCheckInfo"
            android:label="Check Info"/>
        <activity android:name="com.pos.swoop.ICCardReader" />
        <activity android:name="com.pos.swoop.MSReader" />
        <activity
            android:name="com.pos.swoop.AddSeal"
            android:label="Seal Info"
            android:windowSoftInputMode="adjustPan"/>
        <activity
            android:name="com.pos.swoop.SellarInfoActivity"
            android:label="Seller Info" />
        <activity
            android:name="com.pos.swoop.SellItemsActivity"
            android:label="Sell Items" />
        <activity
            android:name="com.pos.swoop.BuyOnBoardItemsActivity"
            android:label="Sell Items"
            android:windowSoftInputMode="adjustPan"/>
        <activity
            android:name="com.pos.swoop.PaymentMethodsActivity"
            android:label="Payments"
            android:windowSoftInputMode="adjustPan"/>
        <activity
            android:name="com.pos.swoop.InventoryReportActivity"
            android:label="Print Inventory Reports" />
        <activity
            android:name="com.pos.swoop.VerifyInventoryActivity"
            android:label="Select Service Type" />
        <activity
            android:name="com.pos.swoop.CheckInventoryActivity"
            android:label="Verify Drawers" />
        <activity
            android:name="com.pos.swoop.VerifyDrawerActivity"
            android:label="Verify Drawers" />
        <activity
            android:name="com.pos.swoop.CloseFlightActivity"
            android:label="Close Flight" />
        <activity
            android:name="com.pos.swoop.BuyItemFromCategoryActivity"
            android:label="Sell Items" />
        <activity
            android:name="com.pos.swoop.ConfigureFlightActivity"
            android:label="Configure Flight"
            android:windowSoftInputMode="adjustPan"/>
        <activity
            android:name="com.pos.swoop.VerifyFlightByAdminActivity"
            android:label="POS Admin verifications" />
        <activity
            android:name="com.pos.swoop.MessageToBondActivity"
            android:label="Send a Message to Bond"
            android:windowSoftInputMode="adjustPan"/>
        <activity
            android:name="com.pos.swoop.ExchangeRateActivity"
            android:label="Available Exchange Rates" />
        <activity
            android:name="com.pos.swoop.PreOrderDeliveryActivity"
            android:label="Pre Order Delivery" />
        <activity
            android:name="com.pos.swoop.DefineCartNumbersActivity"
            android:label="Save Cart Numbers" />
        <activity
            android:name="com.pos.swoop.VerifyCartsActivity"
            android:label="Verify Carts" />
        <activity android:name="com.pos.swoop.SelectModeActivity" />
        <activity
            android:name="com.pos.swoop.VoidOrderActivity"
            android:label="Change Order"
            android:windowSoftInputMode="adjustPan"/>
        <activity
            android:name="com.pos.swoop.ClosingSealsActivity"
            android:label="Closing Seals"
            android:windowSoftInputMode="adjustPan"/>
        <activity
            android:name="com.pos.swoop.AnnouncementActivity"
            android:label="Announcements" />
        <activity android:name="com.pos.swoop.NFCReader">
            <intent-filter>
                <action android:name="android.nfc.action.NDEF_DISCOVERED" />

                <category android:name="android.intent.category.DEFAULT" />

                <data android:mimeType="text/plain" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.pos.swoop.LoadPreOrderAdminActivity"
            android:label="Pack Pre Orders"
            android:windowSoftInputMode="adjustPan"/>
        <activity
            android:name="com.pos.swoop.SelectPreOrderTypeActivity"
            android:label="Select pre order type" />
        <activity
            android:name="com.pos.swoop.AcceptPreOrdersActivity"
            android:label="Accept pre orders"
            android:windowSoftInputMode="adjustPan"/>
        <activity
            android:name="com.pos.swoop.PreOrderPaymentsActivity"
            android:label="Pre order payments"
            android:windowSoftInputMode="adjustPan"/>
        <activity
            android:name="com.pos.swoop.PrintInventorActivity"
            android:label="Print Inventory" />
        <activity
            android:name="com.pos.swoop.SelectServiceTypeForSealActivity"
            android:label="Select Service type" />
        <activity android:name="com.pos.swoop.UploadSalesDataActivity" />
        <activity android:name="com.pos.swoop.SelectServiceTypeForPreOrdersActivity" />
        <activity android:name="com.pos.swoop.ScannerMainActivity"></activity>
    </application>

</manifest>